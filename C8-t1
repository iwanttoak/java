
public class Xulong {

	private String title ,type;
	private int page;
	
	Xulong(String cc, int page) {
		super();
		this.title = cc;
		this.type = "计算机";
		this.page = page;
	}
	
	Xulong(String title, String type, int page) {
		super();
		this.title = title;
		this.type = type;
		this.page = page;
	}
	
	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public int getPage() {
		return page;
	}

	public void setPage(int page) {
		this.page = page;
	}

	void Showinfo()
	{
			System.out.println("名称：        "+title);
			System.out.println("页数：        "+page);
			System.out.println("种类：        "+type);
	}

}
